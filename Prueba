function formatFieldWithLineBreaks(field) {
  // Verifica si 'field' es null o undefined
  if (field == null) {
    return ""; // O algÃºn valor por defecto adecuado
  }
  
  return field.split("\n").map((line, index) => (
    <React.Fragment key={index}>
      {line}
      <br />
    </React.Fragment>
  ));
}